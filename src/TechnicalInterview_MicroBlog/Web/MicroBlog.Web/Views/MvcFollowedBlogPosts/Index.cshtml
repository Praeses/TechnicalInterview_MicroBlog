@model IEnumerable<MicroBlog.Web.Models.ViewModel.FollowedBlogPostViewModel>
@{
    ViewBag.Title = "Followed Blog Post Feed";
}

<h2>@ViewBag.Title.</h2>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <h2>@Html.DisplayFor(modelItem => item.Title)</h2>
                <h4>@Html.DisplayFor(modelItem => item.Content)</h4>
                <p class="text-muted">Authored by: @Html.DisplayFor(modelItem => item.UserName)</p>
            </td>
        </tr>
    }
</table>